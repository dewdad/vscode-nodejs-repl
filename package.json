{
    "name": "nodejs-repl",
    "displayName": "Node.js REPL",
    "description": "Node.js Interactive window (REPL)",
    "version": "0.4.1",
    "publisher": "lostfields",
    "preview": true,
    "engines": {
        "vscode": "^1.19.0"
    },
    "license": "MIT",
    "categories": [
        "Languages",
        "Other"
    ],
    "keywords": [
        "node",
        "node.js",
        "nodejs",
        "repl",
        "interactive"
    ],
    "icon": "icon.png",
    "bugs": {
        "url": "https://github.com/lostfields/vscode-nodejs-repl/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lostfields/vscode-nodejs-repl.git"
    },
    "activationEvents": [
        "onCommand:extension.nodejsRepl"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.nodejsRepl",
            "title": "Node.js Interactive window (REPL)"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}